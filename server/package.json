{
    "name": "food-delivery-be",
    "version": "1.0.0",
    "description": "Food delivery backend API with Express and TypeScript",
    "main": "dist/index.js",
    "scripts": {
        "dev": "nodemon src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "setup": "npm install && npx prisma migrate dev && npm run db:seed",
        "postinstall": "prisma generate",
        "db:migrate": "prisma migrate dev",
        "db:generate": "prisma generate",
        "db:studio": "prisma studio",
        "db:seed": "ts-node prisma/seed.ts",
        "db:reset": "prisma migrate reset --force",
        "db:deploy": "prisma migrate deploy"
    },
    "keywords": [
        "express",
        "typescript",
        "food-delivery",
        "api"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@payos/node": "^2.0.4",
        "@prisma/client": "5.22.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0",
        "pg": "^8.17.2",
        "prisma": "5.22.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^30.0.0",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.10.5",
        "@types/pg": "^8.16.0",
        "@types/supertest": "^6.0.3",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "jest": "^30.2.0",
        "nodemon": "^3.0.2",
        "supertest": "^7.2.2",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}
